<p id="notice"><%= notice %></p>

<p>
  <strong>Crawler session:</strong>
  <%= @url_history.crawler_session.id %>
</p>

<p>
  <strong>Url:</strong>
  <%= @url_history.url %>
</p>

<p>
  <strong>Image path:</strong>
  <%= @url_history.image_path %>
</p>

<p>
  <strong>Diff to previous:</strong>
  <%= @url_history.diff_to_previous %>
</p>

<p>
  <%= link_to 'Edit', edit_url_history_path(@url_history), class: "btn btn-success" %>
  <%= link_to 'Destroy', @url_history, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
</p>

<%= link_to 'Back', @url_history.crawler_session, class: "btn btn-default" %>
